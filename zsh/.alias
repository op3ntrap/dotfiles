#SYSTEM
alias ll='ls -l'
alias rm='rm -i'
alias df='df -h'
alias chowner='sudo chown risan:risan'
alias du='du -h'
alias rr='ranger'
alias surr='sudo ranger'
alias v='nvim'
alias vim='/bin/nvim'
alias nvim='/bin/nvim'
alias emby-start='systemctl start emby-server.service'
alias emby-stop='systemctl stop emby-server.service'
alias gg='googler'
alias btstart='systemctl enable bluetooth.service && systemctl start bluetooth.service'
alias btstop='systemctl disable bluetooth.service'
alias powertop='sudo powertop'
alias suvi='sudo nvim'
alias s='cat ~/pass | sudo -S nvim'
alias ls='exa'
alias la='exa -a'
alias brb='source ~/.zshrc'
alias swap-sv='rm -R $HOME/.cache/SpaceVim/swap'
alias swap-nv='rm -R $HOME/.cache/nvim/swap'
alias ownit='sudo chmod -R 777'
#DIRECTORY
alias ..='cd ..'
alias ...='cd ../../../'
alias ....='cd ../../../../'
